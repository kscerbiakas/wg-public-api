{
  "compilerOptions": {
	"declaration": true,
	"module": "commonjs",
	"target": "es5",
	"sourceMap": false,
	"outDir": "lib",
	"baseUrl": ".",
	"paths": {
	  "@interfaces/*": [
		"src/interfaces/*"
	  ],
	  "@services/*": [
		"src/services/*",
	  ],
	  "@errors/*": [
		"src/errors/*"
	  ],
	  "@clients/*": [
		"src/clients/*"
	  ]
	}
  },
  "include": [
	"src"
  ],
  "exclude": [
	"lib",
	"node_modules"
  ],
}
